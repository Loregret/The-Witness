		{
			"id": "sarah1",
			"dialogs": [
				{
					"char": "player",
					"line": "Sarah."
				},
				{
					"char": "npc",
					"line": "You're late, again."
				},
				{
					"char": "player",
					"line": "So what?"
				},
				{
					"char": "npc",
